{
  "parser"  : "babel-eslint",
  "plugins": [
    "import"
  ],
  "extends" : ["airbnb"],
  "rules": {
    "comma-dangle": 0,
    "no-new": 0,
    "strict": 0,
    "generator-star-spacing": 0,
    "camelcase": 0,
    "no-console": 0,
    "consistent-return": 0,
    "func-names": 0,
    "curly": 0,
    "no-constant-condition": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "no-unreachable": 0,
    "spaced-comment": 0,
    "default-case": 0,
    "new-cap": [2, {"capIsNew": false, "newIsCap": true}],
    "no-floating-decimal": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": [2, "nofunc"],
    "object-curly-spacing": [2, "never"],
    "object-shorthand": 0,
    "padded-blocks": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/no-multi-comp": 0,
    "react/sort-comp": 0,
    "import/no-unresolved": [2, {"commonjs": true, "amd": true}],
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2
  },
  "globals": {
    "after": false,
    "afterEach": false,
    "before": false,
    "beforeEach": false,
    "describe": false,
    "it": false,
    "require": false
  },
  "settings": {
    "import/ignore": [
      "node_modules",
      "\\.svg$",
      "\\.png",
      "\\.jpeg",
      "\\.jpg",
      "\\.json$"
    ],
    "import/parser": "babel-eslint",
    "import/resolve": {
      "extensions": [
        ".js",
        ".jsx",
        ".json"
      ]
    }
  }
}
